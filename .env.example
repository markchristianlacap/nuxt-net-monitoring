# PfSense Config
NUXT_SNMP_HOST=127.0.0.1
NUXT_SNMP_COMMUNITY=hello@world

# Interface Configuration (optional - will auto-detect first interface if not set)
# Specify single interface name
NUXT_SNMP_INTERFACE=wan
# Or specify multiple interfaces (comma-separated)
# NUXT_SNMP_INTERFACE=wan,lan,em0

# Ping Config (supports multiple hosts separated by comma)
NUXT_PING_HOST=8.8.8.8,1.1.1.1

# DB Config
NUXT_DB_HOST=127.0.0.1
NUXT_DB_PORT=5432
NUXT_DB_USER=postgres
NUXT_DB_PASSWORD=postgres
NUXT_DB_NAME=net-monitor

# Nuxt Basic Auth
NUXT_USER=admin
NUXT_PASS=secret123
