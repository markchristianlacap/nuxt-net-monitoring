# PfSense Config
NUXT_SNMP_HOST=127.0.0.1
NUXT_SNMP_COMMUNITY=hello@world

# Interface Configuration (choose one method):
# Method 1: Specify interface name (easier, recommended)
NUXT_SNMP_INTERFACE=wan

# Method 2: Specify OIDs directly (advanced, overrides interface name if set)
# NUXT_SNMP_IN_OID=1.3.6.1.2.1.2.2.1.10.5
# NUXT_SNMP_OUT_OID=1.3.6.1.2.1.2.2.1.16.5

# Ping Config (supports multiple hosts separated by comma)
NUXT_PING_HOST=8.8.8.8,1.1.1.1

# DB Config
NUXT_DB_HOST=127.0.0.1
NUXT_DB_PORT=5432
NUXT_DB_USER=postgres
NUXT_DB_PASSWORD=postgres
NUXT_DB_NAME=net-monitor

# Nuxt Basic Auth
NUXT_USER=admin
NUXT_PASS=secret123
